{
  "services": [
    {
      "name": "avnav",
      "image": "ghcr.io/hatlabs/avnav-docker:20250822",
      "isMain": true,
      "internalPort": "8080",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/data"
        },
        {
          "hostPath": "${APP_DATA_DIR}/charts",
          "containerPath": "/charts"
        },
        {
          "hostPath": "${APP_DATA_DIR}/config",
          "containerPath": "/config"
        },
        {
          "hostPath": "/dev",
          "containerPath": "/dev"
        }
      ],
      "extraHosts": [
        "host.docker.internal:host-gateway"
      ],
      "environment": {}
    }
  ]
}
