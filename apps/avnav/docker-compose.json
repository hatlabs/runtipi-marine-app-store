{
  "services": [
    {
      "name": "avnav",
      "image": "xfreex/avnav-daily:20250901",
      "isMain": true,
      "internalPort": "8080",
      "networkMode": "host",
      "privileged": true,
      "user": "1000:1000",
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/var/lib/avnav"
        },
        {
          "hostPath": "/dev",
          "containerPath": "/dev"
        }
      ],
      "environment": {
        "UID": 1000,
        "GID": 1000
      }
    }
  ]
}
